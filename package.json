{
  "name": "vue-fes-component-test-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "echo '{ \"posts\": [] }' > db.json",
    "serve": "vue-cli-service serve",
    "api": "json-server --watch db.json --port 8000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit --include tests/unit/init.js",
    "test:unit:dev": "vue-cli-service test:unit --include tests/unit/init.js --watch",
    "test:unit-karma": "karma start --single-run",
    "test:unit-karma:dev": "karma start"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-plugin-unit-mocha": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.25",
    "chai": "^4.1.2",
    "flush-promises": "^1.0.0",
    "json-server": "^0.14.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-nightmare": "^0.4.15",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "rosie": "^2.0.1",
    "sinon": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  }
